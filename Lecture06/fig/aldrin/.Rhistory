function(U=NULL, df_n=100, df_d = 100, curveColor=1, border=1, col='#CCCCCC', xlim=NULL, ylim=NULL, xlab='', ylab='', detail=999){#
	if(is.null(xlim)[1]){#
		xlim <- c(0,U+0.01*U)#
	}#
	temp <- diff(range(xlim))#
	x    <- seq(xlim[1] - temp/4, xlim[2] + temp/4, length.out=detail)#
	y    <- df(x, df_n, df_d)#
	if(is.null(ylim)[1]){#
		ylim <- range(c(0,y))#
	}#
	plot(x, y, type='l', xlim=xlim, ylim=ylim, xlab=xlab, ylab=ylab, axes=FALSE, col=curveColor, ...)#
	if(!is.null(L[1])){#
		these <- (x <= L)#
		X <- c(x[these][1], x[these], rev(x[these])[1])#
		Y <- c(0, y[these], 0)#
		polygon(X, Y, border=border, col=col)#
	}#
	if(!is.null(U[1])){#
		these <- (x >= U)#
		X <- c(x[these][1], x[these], rev(x[these])[1])#
		Y <- c(0, y[these], 0)#
		polygon(X, Y, border=border, col=col)#
	}#
	if(all(!is.null(M[1:2]))){#
		these <- (x >= M[1] & x <= M[2])#
		X <- c(x[these][1], x[these], rev(x[these])[1])#
		Y <- c(0, y[these], 0)#
		polygon(X, Y, border=border, col=col)#
	}#
	abline(h=0)#
}
